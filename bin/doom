#!/Users/keviryan/Documents/straylight/doom_cli/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'doom-cli','console_scripts','doom'
__requires__ = 'doom-cli'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('doom-cli', 'console_scripts', 'doom')()
    )
